<!DOCTYPE html>
<html>
<head>
  <title>ClientRecommend</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
<% if session[:asecret] && session[:atoken]%>
<%= link_to "Logout", logout_url %>
<% else %>
<%= link_to :login, linkedin_login_path%>
<% end%>
<%= yield %>

</body>
</html>
